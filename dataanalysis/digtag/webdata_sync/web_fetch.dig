timezone: Asia/Tokyo

schedule:
  daily>: 07:00:00

_export:
  workflow_name: "useractionlog"
  start_msg:     "digdag ${workflow_name} start"
  end_msg:       "digdag ${workflow_name} finish"
  error_msg:     "digdag ${workflow_name} error"

+strat:
  echo>: ${start_msg}

+transform:
  sh>: spark-submit webanalyzer.py

+load:
  embulk>: ./web_load.yml

+end:
  echo>: ${end_msg}

_error:
  echo>: ${error_msg}
